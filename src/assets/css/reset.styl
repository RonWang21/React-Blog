/* minireset.css v0.0.5 | MIT License | github.com/jgthms/minireset.css */
html, body, p, ol, ul, li, dl, dt, dd, blockquote, figure, fieldset, legend, textarea, pre, iframe, hr, h1, h2, h3, h4, h5, h6
  margin 0
  padding 0
h1, h2, h3, h4, h5, h6
  font-size 100%
  font-weight normal
a
  text-decoration none
  cursor pointer
ul
  list-style none
button, input, select, textarea
  margin 0
html
  box-sizing border-box
*, *:before, *:after
  box-sizing inherit
img, video
  height auto
  max-width 100%
iframe
  border 0
table
  border-collapse collapse
  border-spacing 0
td, th
  padding 0
  text-align left
html, body
  height 100%
  background-color rgb(244, 245, 245)
// UI组件---消息提示框
.el-message-content
  div
    .el-message
      min-width 200px !important
      top 80px !important
      left 85% !important
#root
  height 100%
  min-width 400px
  min-height 400px
